{
  "manifest_version": 3,
  "name": "ModeSwitch",
  "version": "1.0.0",
  "description": "Crea modos de navegación y abre todas tus pestañas favoritas con 1 clic.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
